<table class="ui table">
      <thead>
        <tr>
          <th>Title</th>
          <th>Link</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        {{#each bookmark.bookmarks}}
          <tr>
            <td>
              {{title}}
            </td>
            <td>
              <a href={{link}} target="_blank">{{title}}</a>
            </td>
            <td>
              {{description}}
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>