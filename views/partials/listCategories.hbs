{{#each bookmarks}}
<section class="ui stacked segment">
<h2 class = "ui header">
  {{Category}}
  </h2>
  <a href = "/bookmark/{{id}}" class="ui icon button">
  <i class="icon folder open"></i></a>
  
  <a href="/dashboard/deleteCategory/{{id}}" class = "ui icon button">
  <i class="icon trash"></i>
  </a>
  
</section>
{{/each}}